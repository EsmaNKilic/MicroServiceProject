  client:
    fetchRegistry: true
    registerWithEureka: true
    serviceUrl:
      defaultZone: http://localhost:9001/eureka
  instance:
    instance-id: ${spring.application.name}:${instanceId:${random.value}}
    preferIpAddress: true
server:
  port: 0
spring:
  application:
    name: filter
  autoconfigure:
    exclude: org.springframework.boot.autoconfigure.jdbc.DataSourceAutooConfiguration
  data:
    mongodb:
      database: rentacarfilterdb
      host: localhost
      port: 27017
