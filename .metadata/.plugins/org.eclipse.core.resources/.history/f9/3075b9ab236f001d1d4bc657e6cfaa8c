package com.kodlamaio.inventoryservice.entities;

@Data
@NoArgsConstructor
@AllArgsConstructor
@Entity
@Table(name="models")
public class Model {
	@Id
	@Column(name = "id")
	private String id;
	
	@Column(name = "name")
	private String name;
	
	@ManyToOne
	@JoinColumn(name = "brand_id")
	private Brand brand;
	
	@Column(mappedBy = "model")
	List<Car> cars;
}
